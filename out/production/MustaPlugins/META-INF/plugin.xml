<idea-plugin>
    <id>com.your.company.unique.plugin.id</id>
    <name>MustaPlugin</name>
    <version>1.0</version>
    <vendor email="support@yourcompany.com" url="http://www.yourcompany.com">MustaBelmo</vendor>

    <description><![CDATA[
  This plugin removes the annotationss over the methods and moves them back to the fields.
  this is generally useful when using lombok as setters and getters generator.
]]></description>

    <change-notes><![CDATA[
     First version
    ]]>
    </change-notes>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="173.0"/>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <!-- uncomment to enable plugin in all products
    <depends>com.intellij.modules.lang</depends>
    -->

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <!-- Add your actions here -->
        <action id="musta.belmo.hibernate.annotation.remover.LombokifyAction"
                class="musta.belmo.hibernate.annotation.remover.LombokifyAction" text="Lombokify"
                description="Turn into lombok class">
            <add-to-group group-id="ToolsMenu" anchor="after" relative-to-action="musta.belmo.hibernate.annotation.remover.PopupDialogAction"/>
        </action>

        <action id="musta.belmo.hibernate.annotation.remover.PopupDialogAction"
                class="musta.belmo.hibernate.annotation.remover.PopupDialogAction" text="Move JPA annotations to fields"
                description="description">
            <add-to-group group-id="ToolsMenu" anchor="first" />
        </action>
    </actions>

</idea-plugin>